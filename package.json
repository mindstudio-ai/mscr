{
  "name": "mscr",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "generate-schemas": "tsx src/cli/generate-schemas.ts",
    "validate": "tsx src/cli/validate.ts",
    "build": "tsx src/cli/build-registry.ts",
    "test": "vitest run --reporter=default",
    "lint": "eslint \"**/*.{ts,js,cjs}\"",
    "lint:fix": "eslint \"**/*.{ts,js,cjs,yml}\" --fix",
    "format": "prettier --write \"**/*.{ts,js,cjs,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,js,cjs,yml}\"",
    "prebuild": "npm run lint:fix && npm run format && npm run validate",
    "start:inbound-server": "tsx src/cli/inbound-server.ts",
    "build:sync-local": "npm run build && curl -X POST http://localhost:3129/v1/integrations/connector-registry/_hooks/sync -H \"Content-Type: application/json\" --data-binary @dist/registry.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^5.0.3",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.6.0",
    "dotenv": "^17.2.1",
    "dotenv-expand": "^12.0.2",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "mime-types": "^3.0.1",
    "mkdirp": "^3.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-curly": "^0.3.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.12.2",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@notionhq/client": "^4.0.2",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2"
  }
}
